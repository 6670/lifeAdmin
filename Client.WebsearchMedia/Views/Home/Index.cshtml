@model IEnumerable<Client.WebsearchMedia.Models.TransferCommonLeads>

@{
    ViewBag.Title = "Index";
    Layout = "~/views/shared/_layoutdesboard.cshtml";
}

<div class="container-fluid">
    <!-- Page Heading -->
    <div class="row" id="main">
        <div class="col-sm-12 col-md-12 well" id="content">
            <h1>Welcome Admin!</h1>
            <table id="example" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%">
                <thead>
                    <tr>
                        <th>LeadID</th>
                        <td>Edit</td>
                        <th>GeneratedTime</th>
                        <th>TransferTime</th>
                        <th>Buyer</th>
                        <th>Product</th>
                        <th>Status</th>
                        <th>Source</th>
                        <th>Keyword</th>
                        <th>MatchType</th>
                        <th>FullName</th>
                        
                        <th>HomePhone</th>
                        <th>Email</th>
                        <th>Address</th>
                        <th>Descriptions</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach(var i in Model.OrderByDescending(m=>m.CommonLeadId))
                    {
                        <tr>
                          @*  <td><span> | </span>@Html.ActionLink(i.CommonLeadId.ToString(), "Edit", new { id =@i.CommonLeadId })</td>*@
                           <td>@i.CommonLeadId</td>
                            <td>@Html.ActionLink(i.CommonLeadId.ToString(), "Edit", new { id = @i.CommonLeadId })</td>
                             <td>@i.GeneratedDateTime</td>
                            <td>@i.TransferDateTime</td>
                            <td>@i.BuyerName</td>
                            <td>@i.ProductName</td>
                            <td>@i.Status</td>
                            <td>@i.Source</td>
                            <td>@i.Keyword</td>
                            <td>@i.MatchType</td>
                            <td>@i.FullName</td>
                            <td>@i.HomePhone</td>
                            <td>@i.Email</td>
                            <td>@i.Address</td>
                            <td><textarea style="width:1000px;">@i.Descriptions</textarea></td>
                        
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
    <!-- /.row -->
</div>

<script type="text/javascript">
        $(function () {
            $('#example').DataTable( );
            $('[data-toggle="tooltip"]').tooltip();
            $(".side-nav .collapse").on("hide.bs.collapse", function () {
                $(this).prev().find(".fa").eq(1).removeClass("fa-angle-right").addClass("fa-angle-down");
            });
            $('.side-nav .collapse').on("show.bs.collapse", function () {
                $(this).prev().find(".fa").eq(1).removeClass("fa-angle-down").addClass("fa-angle-right");
            });
        })
    </script>