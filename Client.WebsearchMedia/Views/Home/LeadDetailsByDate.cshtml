@model IEnumerable<EntityObjects.Objects.CommonLead>

@{
    ViewBag.Title = "Index";
    Layout = "~/views/shared/_layoutdesboard.cshtml";
}

<div class="container-fluid">
    <!-- Page Heading -->
    <div class="row" id="main">
        <div class="col-sm-12 col-md-12 well" id="content">
            <div class="row">
                <div class="col-md-6">
                    <h1>Welcome Admin!</h1>
                </div>
                <div class="col-md-6">
            @using (Html.BeginForm("LeadDetailsByDate", "home", FormMethod.Post))
            { <select class="form-group" name="dateRange">
                        <option value="1">ToDay</option>
                        <option value="2">YesterDay</option>
                        <option selected value="3">This Week</option>
                        <option value="4">Last Week</option>
                       <option value="5">This Month</option>
                       <option value="6">Last Month</option>


                    </select>
                    <input type="submit" class="btn-default" name="Submit" value="Submit" />
                    }
                </div>
            </div>
            <table id="leadByDateDetals" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%">
                <thead>
                    <tr>
                        <th>
                            GeneratedDateTime
                        </th>
                        <th>
                            UpdatedDateTime
                        </th>
                        <th>
                            Title
                        </th>
                        <th>
                            FirstName
                        </th>
                        <th>
                            LastName
                        </th>
                        <th>
                            HomePhone
                        </th>
                        <th>
                            WorkPhone
                        </th>
                        <th>
                            PostCode
                        </th>
                        <th>
                            Email
                        </th>
                        <th>
                            Address
                        </th>
                        <th>
                            model.City
                        </th>
                        <th>
                            ProductName
                        </th>
                        <th>
                            SiteId
                        </th>
                        <th>
                            Status
                        </th>
                        <th>
                            Source
                        </th>
                        <th>
                            Keyword
                        </th>
                        <th>
                            MatchType
                        </th>
                        <th>
                            IpAddress
                        </th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @item.GeneratedDateTime
                            </td>
                            <td>
                                @item.UpdatedDateTime
                            </td>
                            <td>
                                @item.Title
                            </td>
                            <td>
                                @item.FirstName
                            </td>
                            <td>
                                @item.LastName
                            </td>
                            <td>
                                @item.HomePhone
                            </td>
                            <td>
                                @item.WorkPhone
                            </td>
                            <td>
                                @item.PostCode
                            </td>
                            <td>
                                @item.Email
                            </td>
                            <td>
                                @item.Address
                            </td>
                            <td>
                                @item.City
                            </td>
                            <td>
                                @item.ProductName
                            </td>
                            <td>
                                @item.SiteId
                            </td>
                            <td>
                                @item.Status
                            </td>
                            <td>
                                @item.Source
                            </td>
                            <td>
                                @item.Keyword
                            </td>
                            <td>
                                @item.MatchType
                            </td>
                            <td>
                                @item.IpAddress
                            </td>
                        </tr>

                    }
                </tbody>
            </table>
        </div>
    </div>
    <!-- /.row -->
</div>
<script type="text/javascript">
    $(function () {
        $('#leadByDateDetals').DataTable();
        $('[data-toggle="tooltip"]').tooltip();
        $(".side-nav .collapse").on("hide.bs.collapse", function () {
            $(this).prev().find(".fa").eq(1).removeClass("fa-angle-right").addClass("fa-angle-down");
        });
        $('.side-nav .collapse').on("show.bs.collapse", function () {
            $(this).prev().find(".fa").eq(1).removeClass("fa-angle-down").addClass("fa-angle-right");
        });
    });
</script>